    "url": "https://github.com/rayGreycloud/another-react-express-starter/issues"
  },
  "homepage": "https://github.com/rayGreycloud/another-react-express-starter#readme",
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "nodemon": "^1.14.7"
  }
}
